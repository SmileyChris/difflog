{
  "name": "difflog",
  "private": true,
  "version": "3.4.0",
  "cliVersion": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run dev:migrate && vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "deploy": "bun run build && wrangler pages deploy .svelte-kit/cloudflare",
    "db:create": "wrangler d1 create difflog-db",
    "db:migrate": "wrangler d1 migrations apply difflog-db --remote",
    "dev:migrate": "wrangler d1 migrations apply difflog-db --local --persist-to=.wrangler/state",
    "docs": "uvx zensical serve",
    "cleanup": "bun run scripts/cleanup-profiles.ts",
    "test": "bun test",
    "cli": "bun src/cli/main.ts",
    "cli:build": "bun build src/cli/main.ts --compile --outfile difflog"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@sveltejs/adapter-cloudflare": "^7.2.6",
    "@sveltejs/kit": "^2.50.2",
    "@sveltejs/vite-plugin-svelte": "^6.2.4",
    "svelte": "^5.49.1",
    "svelte-check": "^4.3.6",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wrangler": "^4.61.0"
  },
  "dependencies": {
    "cross-keychain": "^1.1.0"
  }
}
